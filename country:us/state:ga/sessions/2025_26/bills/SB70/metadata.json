  "identifier": "SB 70",
  "title": "\"Conyers Community Improvement Districts Act\"; enact",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "description",
      "abstract": "A BILL to be entitled an Act to provide for the creation of one or more community improvement districts within the City of Conyers, Georgia; to provide for a short title; to provide for the purpose of such districts; to provide for subsequent issues thereof; to provide for construction; to provide that no notice, proceeding, publication, or referendum shall be required; to provide for procedures connected with all of the foregoing; to provide for the dissolution and reactivation of districts under certain conditions; to provide for related matters; to repeal conflicting laws; and for other purposes."
    }
  ],
