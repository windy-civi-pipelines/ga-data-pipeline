{
  "legislative_session": "2025_26",
  "identifier": "SB 141",
  "title": "Revenue and Taxation; the appeal and protest period from 30 days to 45 days for tax assessments; extend",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "description",
      "abstract": "A BILL to be entitled an Act to amend Title 48 of the Official Code of Georgia Annotated, relating to revenue and taxation, so as to extend the appeal and protest period from 30 days to 45 days for tax assessments; to clarify final determination language for federal income tax adjustments; to provide for related matters; to provide for an effective date and applicability; to repeal conflicting laws; and for other purposes."
    }
  ],
