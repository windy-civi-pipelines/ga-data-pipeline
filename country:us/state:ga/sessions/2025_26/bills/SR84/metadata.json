{
  "legislative_session": "2025_26",
  "identifier": "SR 84",
  "title": "Right to Reproductive Freedom; provide",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "resolution"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "description",
      "abstract": "A RESOLUTION proposing an amendment to the Constitution so as to provide that every individual has a fundamental right to reproductive freedom and such right shall not be denied, burdened, or infringed upon unless justified by a compelling state interest; to limit the purpose of a compelling state interest; to provide that the state may regulate the provision of abortion care after fetal viability; to provide for a definition; to provide for severability; to provide for related matters; to provide for submission of this amendment for ratification or rejection; and for other purposes."
    }
  ],
