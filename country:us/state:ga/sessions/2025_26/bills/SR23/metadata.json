  "subject": [],
  "abstracts": [
    {
      "note": "description",
      "abstract": "A RESOLUTION applying to the United States Congress to call a convention under Article V of the Constitution of the United States limited to proposing an amendment to the Constitution of the United States to set a limit on the number of terms that a person may be elected as a member of the United States House of Representatives and to set a limit on the number of terms that a person may be elected as a member of the United States Senate; and for other purposes."
    }
  ],
