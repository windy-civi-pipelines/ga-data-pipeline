  "subject": [],
  "abstracts": [
    {
      "note": "description",
      "abstract": "A RESOLUTION authorizing the conveyance of certain state owned property located in Bryan County, Colquitt County, Fulton County, Glynn County, Habersham County, Hall County, and Henry County; authorizing the lease of certain state owned property located in Cherokee County, Effingham County, Hall County, Miller County, Sumter County, and Tattnall County; authorizing the exchange of certain state owned property located in Clarke County; to provide an effective date; to provide for related matters; to repeal conflicting laws; and for other purposes."
    }
  ],
