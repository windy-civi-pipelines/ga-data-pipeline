  "legislative_session": "2025_26",
  "identifier": "SB 260",
  "title": "Central State Hospital Redevelopment Authority; create",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "description",
      "abstract": "A BILL to be entitled an Act to amend Chapter 7 of Title 50 of the O.C.G.A., relating to Department of Economic Development, so as to create the Central State Hospital Redevelopment Authority; to confer powers and impose duties on the authority; to provide for membership and appointment of members of the authority; to provide for terms of the members; to prohibit employees and members from having certain interests; to provide for revenue bonds; to provide for venue and jurisdiction; to provide for the authority's purpose; to provide for rules and regulations; to provide for tax exemptions; to provide for supplemental powers; to provide for liberal construction; to provide for successor interests; to provide for definitions; to provide for related matters; to repeal conflicting laws; and for other purposes."
    }
  ],
